msgstr "CRPS"

msgid "DSS"
msgstr "DSS"

msgid "Log score"
msgstr "Puntuación log"

msgid "Coverage"
msgstr "Cobertura"

msgid "Bias"
msgstr "Sesgo"

msgid "PIT"
msgstr "PIT"

